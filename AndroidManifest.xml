<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.androidians.betapro"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="17" android:maxSdkVersion="17"/>
	<uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/beta_pro_icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.androidians.betapro.Login"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="DeveloperMain"></activity>
        <activity android:name="ReadReviewActivity"></activity>
        <activity android:name="Home"></activity>
        <activity android:name="ReviewerMain"></activity>
        <activity android:name="Register"></activity>
        <activity android:name="PasswordReset"></activity>
        <activity android:name="Help"></activity>
        <activity android:name="doc"></activity>
        
    </application>
    

</manifest>